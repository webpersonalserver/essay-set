<!--index.wxml-->
<view class="app home">
  <view class="home__module" wx:for="{{list}}" wx:key="{{index}}" bindtap="goPage" data-path="{{item.path}}">
    <image class="home__module_icon" src="{{item.icon}}" mode="aspectFit"></image>
    <view class="home__module_info">
      <view class="home__module_info-title">{{item.title}}</view>
      <view class="home__module_info-desc">{{item.desc}}</view>
    </view>
  </view>
</view>
